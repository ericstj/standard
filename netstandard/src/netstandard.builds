<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.props))\dir.props" />
  <ItemGroup>
    <Project Include="netstandard.csproj">
      <TargetGroup>xamarin.ios</TargetGroup>
    </Project>
    <Project Include="netstandard.csproj">
      <TargetGroup>net461</TargetGroup>
    </Project>
    <Project Include="netstandard.csproj" Condition="Exists('$(NETCoreAppPath)')">
      <TargetGroup>netcoreapp1.1</TargetGroup>
    </Project>
    <Project Include="netstandard.csproj" Condition="Exists('$(NETCoreAppPath)')">
      <TargetGroup>netcoreapp1.1.ref</TargetGroup>
    </Project>
  </ItemGroup>
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.traversal.targets))\dir.traversal.targets" />
</Project>

